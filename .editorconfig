# editorconfig.org

root = true

[*]
insert_final_newline = true
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true

[*.cs]
indent_size = 4

[*.sln]
indent_size = 4
indent_style = tab

# Generated code
[*{_AssemblyInfo.cs,.notsupported.cs}]
generated_code = true
